 var loading = ref(false);\n    var orders = ref([]);\n    var stations = ref([]);\n    var currentPage = ref(1);\n    var pageSize = ref(10);\n    var total = ref(0);\n    var detailDialogVisible = ref(false);\n    var currentOrder = ref(null);\n    var filterForm = reactive({\n      orderNo: '',\n      stationId: '',\n      status: '',\n      dateRange: []\n    });\n\n    // 获取订单列表\n    var fetchOrders = function fetchOrders() {\n      loading.value = true;\n\n      // 模拟API请求\n      setTimeout(function () {\n        orders.value = mockOrders;\n        total.value = mockOrders.length;\n        loading.value = false;\n      }, 500);\n    };\n\n    // 获取充电站列表\n    var fetchStations = function fetchStations() {\n      stations.value = mockStations;\n    };\n\n    // 搜索订单\n    var handleSearch = function handleSearch() {\n      loading.value = true;\n\n      // 模拟搜索过滤\n      setTimeout(function () {\n        var filteredOrders = [].concat(mockOrders);\n        if (filterForm.orderNo) {\n          filteredOrders = filteredOrders.filter(function (order) {\n            return order.orderNo.toLowerCase().includes(filterForm.orderNo.toLowerCase());\n          });\n        }\n        if (filterForm.stationId) {\n          filteredOrders = filteredOrders.filter(function (order) {\n            return order.stationId === filterForm.stationId;\n          });\n        }\n        if (filterForm.status) {\n          filteredOrders = filteredOrders.filter(function (order) {\n            return order.status === filterForm.status;\n          });\n        }\n        if (filterForm.dateRange && filterForm.dateRange.length === 2) {\n          var startDate = filterForm.dateRange[0];\n          var endDate = filterForm.dateRange[1];\n          filteredOrders = filteredOrders.filter(function (order) {\n            var orderDate = dayjs(order.startTime).format('YYYY-MM-DD');\n            return orderDate >= startDate && orderDate <= endDate;\n          });\n        }\n        orders.value = filteredOrders;\n        total.value = filteredOrders.length;\n        loading.value = false;\n      }, 500);\n    };\n\n    // 重置筛选条件\n    var resetFilter = function resetFilter() {\n      Object.keys(filterForm).forEach(function (key) {\n        if (Array.isArray(filterForm[key])) {\n          filterForm[key] = [];\n        } else {\n          filterForm[key] = '';\n        }\n      });\n      currentPage.value = 1;\n      fetchOrders();\n    };\n\n    // 查看订单详情\n    var viewOrderDetail = function viewOrderDetail(row) {\n      currentOrder.value = _objectSpread({}, row);\n      detailDialogVisible.value = true;\n    };\n\n    // 取消订单\n    var cancelOrder = function cancelOrder(row) {\n      ElMessageBox.confirm(\"\\u786E\\u5B9A\\u8981\\u53D6\\u6D88\\u8BA2\\u5355 \".concat(row.orderNo, \" \\u5417\\uFF1F\"), '警告', {\n        confirmButtonText: '确定',\n        cancelButtonText: '取消',\n        type: 'warning'\n      }).then(function () {\n        // 模拟取消操作\n        var index = orders.value.findIndex(function (item) {\n          return item.id === row.id;\n        });\n        if (index !== -1) {\n          orders.value[index].status = 'cancelled';\n        }\n        ElMessage.success('订单已取消');\n      })[\"catch\"](function () {\n        // 取消操作\n      });\n    };\n\n    // 格式化日期时间\n    var formatDateTime = function formatDateTime(dateTime) {\n      if (!dateTime) return '-';\n      return dayjs(dateTime).format('YYYY-MM-DD HH:mm:ss');\n    };\n\n    // 计算充电时长\n    var calculateDuration = function calculateDuration(startTime, endTime) {\n      if (!startTime || !endTime) return '-';\n      var start = dayjs(startTime);\n      var end = dayjs(endTime);\n      var diffMinutes = end.diff(start, 'minute');\n      var hours = Math.floor(diffMinutes / 60);\n      var minutes = diffMinutes % 60;\n      return \"\".concat(hours, \"\\u5C0F\\u65F6\").concat(minutes, \"\\u5206\\u949F\");\n    };\n\n    // 获取订单状态类型\n    var getOrderStatusType = function getOrderStatusType(status) {\n      var types = {\n        waiting: 'info',\n        charging: 'primary',\n        completed: 'success',\n        cancelled: 'warning',\n        failed: 'danger'\n      };\n      return types[status] || 'info';\n    };\n\n    // 获取订单状态文本\n    var getOrderStatusText = function getOrderStatusText(status) {\n      var texts = {\n        waiting: '等待中',\n        charging: '充电中',\n        completed: '已完成',\n        cancelled: '已取消',\n        failed: '失败'\n      };\n      return texts[status] || '未知';\n    };\n\n    // 分页处理\n    var handleSizeChange = function handleSizeChange(val) {\n      pageSize.value = val;\n      fetchOrders();\n    };\n    var handleCurrentChange = function handleCurrentChange(val) {\n      currentPage.value = val;\n      fetchOrders();\n    };\n    onMounted(function () {\n      fetchStations();\n      fetchOrders();\n    });\n    return {\n      loading: loading,\n      orders: orders,\n      stations: stations,\n      filterForm: filterForm,\n      currentPage: currentPage,\n      pageSize: pageSize,\n      total: total,\n      detailDialogVisible: detailDialogVisible,\n      currentOrder: currentOrder,\n      handleSearch: handleSearch,\n      resetFilter: resetFilter,\n      viewOrderDetail: viewOrderDetail,\n      cancelOrder: cancelOrder,\n      formatDateTime: formatDateTime,\n      calculateDuration: calculateDuration,\n      getOrderStatusType: getOrderStatusType,\n      getOrderStatusText: getOrderStatusText,\n      handleSizeChange: handleSizeChange,\n      handleCurrentChange: handleCurrentChange\n    };\n  }\n};","map":{"version":3,"names":["ref","reactive","onMounted","ElMessage","ElMessageBox","Search","Refresh","dayjs","mockStations","id","name","mockOrders","orderNo","stationId","stationName","robotCode","startTime","endTime","initialBattery","targetBattery","finalBattery","actualCharge","efficiency","status","notes","components","setup","loading","orders","stations","currentPage","pageSize","total","detailDialogVisible","currentOrder","filterForm","dateRange","fetchOrders","value","setTimeout","length","fetchStations","handleSearch","filteredOrders","concat","filter","order","toLowerCase","includes","startDate","endDate","orderDate","format","resetFilter","Object","keys","forEach","key","Array","isArray","viewOrderDetail","row","_objectSpread","cancelOrder","confirm","confirmButtonText","cancelButtonText","type","then","index","findIndex","item","success","formatDateTime","dateTime","calculateDuration","start","end","diffMinutes","diff","hours","Math","floor","minutes","getOrderStatusType","types","waiting","charging","completed","cancelled","failed","getOrderStatusText","texts","handleSizeChange","val","handleCurrentChange"],"sources":["D:\\software_data\\Project\\Y0527\\CODE_BAK\\frontend\\src\\views\\ChargingOrders.vue"],"sourcesContent":["<template>\r\n  <div class=\"charging-orders\">\r\n    <h1 class=\"page-title\">充电订单</h1>\r\n    \r\n    <!-- 搜索和筛选区域 -->\r\n    <div class=\"filter-container\">\r\n      <el-form :inline=\"true\" :model=\"filterForm\" class=\"filter-form\">\r\n        <el-form-item label=\"订单号\">\r\n          <el-input v-model=\"filterForm.orderNo\" placeholder=\"订单号\" clearable />\r\n        </el-form-item>\r\n        <el-form-item label=\"充电站\">\r\n          <el-select v-model=\"filterForm.stationId\" placeholder=\"选择充电站\" clearable>\r\n            <el-option\r\n              v-for=\"station in stations\"\r\n              :key=\"station.id\"\r\n              :label=\"station.name\"\r\n              :value=\"station.id\"\r\n            />\r\n          </el-select>\r\n        </el-form-item>\r\n        <el-form-item label=\"状态\">\r\n          <el-select v-model=\"filterForm.status\" placeholder=\"订单状态\" clearable>\r\n            <el-option label=\"等待中\" value=\"waiting\" />\r\n            <el-option label=\"充电中\" value=\"charging\" />\r\n            <el-option label=\"已完成\" value=\"completed\" />\r\n            <el-option label=\"已取消\" value=\"cancelled\" />\r\n            <el-option label=\"失败\" value=\"failed\" />\r\n          </el-select>\r\n        </el-form-item>\r\n        <el-form-item label=\"日期范围\">\r\n          <el-date-picker\r\n            v-model=\"filterForm.dateRange\"\r\n            type=\"daterange\"\r\n            range-separator=\"至\"\r\n            start-placeholder=\"开始日期\"\r\n            end-placeholder=\"结束日期\"\r\n            value-format=\"YYYY-MM-DD\"\r\n          />\r\n        </el-form-item>\r\n        <el-form-item>\r\n          <el-button type=\"primary\" @click=\"handleSearch\">\r\n            <el-icon><Search /></el-icon>\r\n            搜索\r\n          </el-button>\r\n          <el-button @click=\"resetFilter\">\r\n            <el-icon><Refresh /></el-icon>\r\n            重置\r\n          </el-button>\r\n        </el-form-item>\r\n      </el-form>\r\n    </div>\r\n    \r\n    <!-- 订单列表 -->\r\n    <el-table\r\n      v-loading=\"loading\"\r\n      :data=\"orders\"\r\n      border\r\n      style=\"width: 100%\"\r\n      row-key=\"id\"\r\n    >\r\n      <el-table-column prop=\"orderNo\" label=\"订单号\" width=\"180\" />\r\n      <el-table-column prop=\"stationName\" label=\"充电站\" width=\"150\" />\r\n      <el-table-column prop=\"robotCode\" label=\"机器人编号\" width=\"120\" />\r\n      <el-table-column label=\"开始时间\" width=\"180\">\r\n        <template #default=\"scope\">\r\n          {{ formatDateTime(scope.row.startTime) }}\r\n        </template>\r\n      </el-table-column>\r\n      <el-table-column label=\"结束时间\" width=\"180\">\r\n        <template #default=\"scope\">\r\n          {{ scope.row.endTime ? formatDateTime(scope.row.endTime) : '-' }}\r\n        </template>\r\n      </el-table-column>\r\n      <el-table-column label=\"充电时长\" width=\"120\">\r\n        <template #default=\"scope\">\r\n          {{ calculateDuration(scope.row.startTime, scope.row.endTime) }}\r\n        </template>\r\n      </el-table-column>\r\n      <el-table-column label=\"充电电量\" width=\"120\">\r\n        <template #default=\"scope\">\r\n          {{ scope.row.actualCharge ? scope.row.actualCharge.toFixed(2) + ' kWh' : '-' }}\r\n        </template>\r\n      </el-table-column>\r\n      <el-table-column label=\"充电效率\" width=\"120\">\r\n        <template #default=\"scope\">\r\n          {{ scope.row.efficiency ? scope.row.efficiency.toFixed(2) + '%' : '-' }}\r\n        </template>\r\n      </el-table-column>\r\n      <el-table-column label=\"状态\" width=\"100\">\r\n        <template #default=\"scope\">\r\n          <el-tag :type=\"getOrderStatusType(scope.row.status)\">\r\n            {{ getOrderStatusText(scope.row.status) }}\r\n          </el-tag>\r\n        </template>\r\n      </el-table-column>\r\n      <el-table-column label=\"操作\" width=\"150\" fixed=\"right\">\r\n        <template #default=\"scope\">\r\n          <el-button size=\"small\" @click=\"viewOrderDetail(scope.row)\">\r\n            详情\r\n          </el-button>\r\n          <el-button \r\n            v-if=\"scope.row.status === 'waiting'\" \r\n            size=\"small\" \r\n            type=\"danger\" \r\n            @click=\"cancelOrder(scope.row)\"\r\n          >\r\n            取消\r\n          </el-button>\r\n        </template>\r\n      </el-table-column>\r\n    </el-table>\r\n    \r\n    <!-- 分页 -->\r\n    <div class=\"pagination-container\">\r\n      <el-pagination\r\n        v-model:currentPage=\"currentPage\"\r\n        v-model:page-size=\"pageSize\"\r\n        :page-sizes=\"[10, 20, 50, 100]\"\r\n        layout=\"total, sizes, prev, pager, next, jumper\"\r\n        :total=\"total\"\r\n        @size-change=\"handleSizeChange\"\r\n        @current-change=\"handleCurrentChange\"\r\n      />\r\n    </div>\r\n    \r\n    <!-- 订单详情对话框 -->\r\n    <el-dialog\r\n      v-model=\"detailDialogVisible\"\r\n      title=\"订单详情\"\r\n      width=\"700px\"\r\n    >\r\n      <div v-if=\"currentOrder\" class=\"order-detail\">\r\n        <el-descriptions :column=\"2\" border>\r\n          <el-descriptions-item label=\"订单号\">{{ currentOrder.orderNo }}</el-descriptions-item>\r\n          <el-descriptions-item label=\"状态\">\r\n            <el-tag :type=\"getOrderStatusType(currentOrder.status)\">\r\n              {{ getOrderStatusText(currentOrder.status) }}\r\n            </el-tag>\r\n          </el-descriptions-item>\r\n          <el-descriptions-item label=\"充电站\">{{ currentOrder.stationName }}</el-descriptions-item>\r\n          <el-descriptions-item label=\"机器人编号\">{{ currentOrder.robotCode }}</el-descriptions-item>\r\n          <el-descriptions-item label=\"开始时间\">{{ formatDateTime(currentOrder.startTime) }}</el-descriptions-item>\r\n          <el-descriptions-item label=\"结束时间\">{{ currentOrder.endTime ? formatDateTime(currentOrder.endTime) : '-' }}</el-descriptions-item>\r\n          <el-descriptions-item label=\"充电时长\">{{ calculateDuration(currentOrder.startTime, currentOrder.endTime) }}</el-descriptions-item>\r\n          <el-descriptions-item label=\"初始电量\">{{ currentOrder.initialBattery ? currentOrder.initialBattery + '%' : '-' }}</el-descriptions-item>\r\n          <el-descriptions-item label=\"目标电量\">{{ currentOrder.targetBattery ? currentOrder.targetBattery + '%' : '-' }}</el-descriptions-item>\r\n          <el-descriptions-item label=\"最终电量\">{{ currentOrder.finalBattery ? currentOrder.finalBattery + '%' : '-' }}</el-descriptions-item>\r\n          <el-descriptions-item label=\"充电电量\">{{ currentOrder.actualCharge ? currentOrder.actualCharge.toFixed(2) + ' kWh' : '-' }}</el-descriptions-item>\r\n          <el-descriptions-item label=\"充电效率\">{{ currentOrder.efficiency ? currentOrder.efficiency.toFixed(2) + '%' : '-' }}</el-descriptions-item>\r\n        </el-descriptions>\r\n        \r\n        <div v-if=\"currentOrder.notes\" class=\"order-notes\">\r\n          <h3>备注信息</h3>\r\n          <p>{{ currentOrder.notes }}</p>\r\n        </div>\r\n      </div>\r\n    </el-dialog>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nimport { ref, reactive, onMounted } from 'vue';\r\nimport { ElMessage, ElMessageBox } from 'element-plus';\r\nimport { Search, Refresh } from '@element-plus/icons-vue';\r\nimport dayjs from 'dayjs';\r\n\r\n// 模拟数据，实际项目中应该从API获取\r\nconst mockStations = [\r\n  { id: '1', name: '一号充电站' },\r\n  { id: '2', name: '二号充电站' }\r\n];\r\n\r\nconst mockOrders = [\r\n  {\r\n    id: '1',\r\n    orderNo: 'CO20230527001',\r\n    stationId: '1',\r\n    stationName: '一号充电站',\r\n    robotCode: 'R001',\r\n    startTime: '2023-05-27T08:30:00',\r\n    endTime: '2023-05-27T10:45:00',\r\n    initialBattery: 15,\r\n    targetBattery: 95,\r\n    finalBattery: 92,\r\n    actualCharge: 5.8,\r\n    efficiency: 91.5,\r\n    status: 'completed',\r\n    notes: '正常完成充电'\r\n  },\r\n  {\r\n    id: '2',\r\n    orderNo: 'CO20230527002',\r\n    stationId: '2',\r\n    stationName: '二号充电站',\r\n    robotCode: 'R002',\r\n    startTime: '2023-05-27T09:15:00',\r\n    endTime: null,\r\n    initialBattery: 20,\r\n    targetBattery: 90,\r\n    finalBattery: null,\r\n    actualCharge: null,\r\n    efficiency: null,\r\n    status: 'charging',\r\n    notes: '充电中'\r\n  }\r\n];\r\n\r\nexport default {\r\n  name: 'ChargingOrders',\r\n  components: {\r\n    Search,\r\n    Refresh\r\n  },\r\n  setup() {\r\n    const loading = ref(false);\r\n    const orders = ref([]);\r\n    const stations = ref([]);\r\n    const currentPage = ref(1);\r\n    const pageSize = ref(10);\r\n    const total = ref(0);\r\n    const detailDialogVisible = ref(false);\r\n    const currentOrder = ref(null);\r\n    \r\n    const filterForm = reactive({\r\n      orderNo: '',\r\n      stationId: '',\r\n      status: '',\r\n      dateRange: []\r\n    });\r\n    \r\n    // 获取订单列表\r\n    const fetchOrders = () => {\r\n      loading.value = true;\r\n      \r\n      // 模拟API请求\r\n      setTimeout(() => {\r\n        orders.value = mockOrders;\r\n        total.value = mockOrders.length;\r\n        loading.value = false;\r\n      }, 500);\r\n    };\r\n    \r\n    // 获取充电站列表\r\n    const fetchStations = () => {\r\n      stations.value = mockStations;\r\n    };\r\n    \r\n    // 搜索订单\r\n    const handleSearch = () => {\r\n      loading.value = true;\r\n      \r\n      // 模拟搜索过滤\r\n      setTimeout(() => {\r\n        let filteredOrders = [...mockOrders];\r\n        \r\n        if (filterForm.orderNo) {\r\n          filteredOrders = filteredOrders.filter(order => \r\n            order.orderNo.toLowerCase().includes(filterForm.orderNo.toLowerCase())\r\n          );\r\n        }\r\n        \r\n        if (filterForm.stationId) {\r\n          filteredOrders = filteredOrders.filter(order => \r\n            order.stationId === filterForm.stationId\r\n          );\r\n        }\r\n        \r\n        if (filterForm.status) {\r\n          filteredOrders = filteredOrders.filter(order => \r\n            order.status === filterForm.status\r\n          );\r\n        }\r\n        \r\n        if (filterForm.dateRange && filterForm.dateRange.length === 2) {\r\n          const startDate = filterForm.dateRange[0];\r\n          const endDate = filterForm.dateRange[1];\r\n          \r\n          filteredOrders = filteredOrders.filter(order => {\r\n            const orderDate = dayjs(order.startTime).format('YYYY-MM-DD');\r\n            return orderDate >= startDate && orderDate <= endDate;\r\n          });\r\n        }\r\n        \r\n        orders.value = filteredOrders;\r\n        total.value = filteredOrders.length;\r\n        loading.value = false;\r\n      }, 500);\r\n    };\r\n    \r\n    // 重置筛选条件\r\n    const resetFilter = () => {\r\n      Object.keys(filterForm).forEach(key => {\r\n        if (Array.isArray(filterForm[key])) {\r\n          filterForm[key] = [];\r\n        } else {\r\n          filterForm[key] = '';\r\n        }\r\n      });\r\n      \r\n      currentPage.value = 1;\r\n      fetchOrders();\r\n    };\r\n    \r\n    // 查看订单详情\r\n    const viewOrderDetail = (row) => {\r\n      currentOrder.value = { ...row };\r\n      detailDialogVisible.value = true;\r\n    };\r\n    \r\n    // 取消订单\r\n    const cancelOrder = (row) => {\r\n      ElMessageBox.confirm(\r\n        `确定要取消订单 ${row.orderNo} 吗？`,\r\n        '警告',\r\n        {\r\n          confirmButtonText: '确定',\r\n          cancelButtonText: '取消',\r\n          type: 'warning'\r\n        }\r\n      ).then(() => {\r\n        // 模拟取消操作\r\n        const index = orders.value.findIndex(item => item.id === row.id);\r\n        if (index !== -1) {\r\n          orders.value[index].status = 'cancelled';\r\n        }\r\n        ElMessage.success('订单已取消');\r\n      }).catch(() => {\r\n        // 取消操作\r\n      });\r\n    };\r\n    \r\n    // 格式化日期时间\r\n    const formatDateTime = (dateTime) => {\r\n      if (!dateTime) return '-';\r\n      return dayjs(dateTime).format('YYYY-MM-DD HH:mm:ss');\r\n    };\r\n    \r\n    // 计算充电时长\r\n    const calculateDuration = (startTime, endTime) => {\r\n      if (!startTime || !endTime) return '-';\r\n      \r\n      const start = dayjs(startTime);\r\n      const end = dayjs(endTime);\r\n      const diffMinutes = end.diff(start, 'minute');\r\n      \r\n      const hours = Math.floor(diffMinutes / 60);\r\n      const minutes = diffMinutes % 60;\r\n      \r\n      return `${hours}小时${minutes}分钟`;\r\n    };\r\n    \r\n    // 获取订单状态类型\r\n    const getOrderStatusType = (status) => {\r\n      const types = {\r\n        waiting: 'info',\r\n        charging: 'primary',\r\n        completed: 'success',\r\n        cancelled: 'warning',\r\n        failed: 'danger'\r\n      };\r\n      return types[status] || 'info';\r\n    };\r\n    \r\n    // 获取订单状态文本\r\n    const getOrderStatusText = (status) => {\r\n      const texts = {\r\n        waiting: '等待中',\r\n        charging: '充电中',\r\n        completed: '已完成',\r\n        cancelled: '已取消',\r\n        failed: '失败'\r\n      };\r\n      return texts[status] || '未知';\r\n    };\r\n    \r\n    // 分页处理\r\n    const handleSizeChange = (val) => {\r\n      pageSize.value = val;\r\n      fetchOrders();\r\n    };\r\n    \r\n    const handleCurrentChange = (va