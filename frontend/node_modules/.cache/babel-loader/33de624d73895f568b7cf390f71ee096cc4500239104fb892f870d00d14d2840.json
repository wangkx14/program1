{"ast":null,"code":"import \"core-js/modules/es.array.concat.js\";\nimport \"core-js/modules/es.array.for-each.js\";\nimport \"core-js/modules/es.array.map.js\";\nimport \"core-js/modules/es.iterator.constructor.js\";\nimport \"core-js/modules/es.iterator.for-each.js\";\nimport \"core-js/modules/es.object.to-string.js\";\nimport \"core-js/modules/es.parse-int.js\";\nimport \"core-js/modules/web.dom-collections.for-each.js\";\nimport { ref, reactive, onMounted, onUnmounted } from 'vue';\nimport { ArrowUp, ArrowDown } from '@element-plus/icons-vue';\nimport * as echarts from 'echarts';\nexport default {\n  name: 'Dashboard',\n  components: {\n    ArrowUp: ArrowUp,\n    ArrowDown: ArrowDown\n  },\n  setup: function setup() {\n    // 图表实例\n    var efficiencyChart = null;\n    var stationStatusChart = null;\n    var orderStatusChart = null;\n    var energyHeatmapChart = null;\n\n    // 时间范围\n    var trendTimeRange = ref('7');\n\n    // 统计数据\n    var stats = reactive({\n      stationCount: 12,\n      stationTrend: 8.3,\n      orderCount: 156,\n      orderTrend: -2.5,\n      avgEfficiency: 91.2,\n      efficiencyTrend: 1.7,\n      activeStations: 10,\n      activeTrend: 0\n    });\n\n    // 初始化图表\n    var initCharts = function initCharts() {\n      initEfficiencyTrendChart();\n      initStationStatusChart();\n      initOrderStatusChart();\n      initEnergyHeatmap();\n    };\n\n    // 初始化充电效率趋势图\n    var initEfficiencyTrendChart = function initEfficiencyTrendChart() {\n      var chartDom = document.getElementById('efficiency-trend-chart');\n      efficiencyChart = echarts.init(chartDom);\n      var option = {\n        tooltip: {\n          trigger: 'axis',\n          formatter: function formatter(params) {\n            var result = params[0].axisValue + '<br/>';\n            params.forEach(function (param) {\n              result += param.marker + ' ' + param.seriesName + ': ' + param.value + '%<br/>';\n            });\n            return result;\n          }\n        },\n 