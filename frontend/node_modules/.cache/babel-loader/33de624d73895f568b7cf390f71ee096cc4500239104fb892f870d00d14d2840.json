9, 0], [3, 10, 5], [3, 11, 4], [3, 12, 7], [3, 13, 14], [3, 14, 13], [3, 15, 12], [3, 16, 9], [3, 17, 5], [3, 18, 5], [3, 19, 10], [3, 20, 6], [3, 21, 4], [3, 22, 4], [3, 23, 1], [4, 0, 1], [4, 1, 3], [4, 2, 0], [4, 3, 0], [4, 4, 0], [4, 5, 1], [4, 6, 0], [4, 7, 0], [4, 8, 0], [4, 9, 2], [4, 10, 4], [4, 11, 4], [4, 12, 2], [4, 13, 4], [4, 14, 4], [4, 15, 14], [4, 16, 12], [4, 17, 1], [4, 18, 8], [4, 19, 5], [4, 20, 3], [4, 21, 7], [4, 22, 3], [4, 23, 0], [5, 0, 2], [5, 1, 1], [5, 2, 0], [5, 3, 3], [5, 4, 0], [5, 5, 0], [5, 6, 0], [5, 7, 0], [5, 8, 2], [5, 9, 0], [5, 10, 4], [5, 11, 1], [5, 12, 5], [5, 13, 10], [5, 14, 5], [5, 15, 7], [5, 16, 11], [5, 17, 6], [5, 18, 0], [5, 19, 5], [5, 20, 3], [5, 21, 4], [5, 22, 2], [5, 23, 0], [6, 0, 1], [6, 1, 0], [6, 2, 0], [6, 3, 0], [6, 4, 0], [6, 5, 0], [6, 6, 0], [6, 7, 0], [6, 8, 0], [6, 9, 0], [6, 10, 1], [6, 11, 0], [6, 12, 2], [6, 13, 1], [6, 14, 3], [6, 15, 4], [6, 16, 0], [6, 17, 0], [6, 18, 0], [6, 19, 0], [6, 20, 1], [6, 21, 2], [6, 22, 2], [6, 23, 6]];\n      var option = {\n        tooltip: {\n          position: 'top',\n          formatter: function formatter(params) {\n            return \"\".concat(params.data[1], \":00 - \").concat(parseInt(params.data[1]) + 1, \":00<br>\").concat(params.data[0] === '0' ? '周一' : params.data[0] === '1' ? '周二' : params.data[0] === '2' ? '周三' : params.data[0] === '3' ? '周四' : params.data[0] === '4' ? '周五' : params.data[0] === '5' ? '周六' : '周日', \"<br>\\u80FD\\u8017: \").concat(params.data[2], \" kWh\");\n          }\n        },\n        grid: {\n          top: '10%',\n          left: '3%',\n          right: '10%',\n          bottom: '10%',\n          containLabel: true\n        },\n        xAxis: {\n          type: 'category',\n          data: hours,\n          splitArea: {\n            show: true\n          }\n        },\n        yAxis: {\n          type: 'category',\n          data: days,\n          splitArea: {\n            show: true\n          }\n        },\n        visualMap: {\n          min: 0,\n          max: 15,\n          calculable: true,\n          orient: 'horizontal',\n          left: 'center',\n          bottom: '0%',\n          inRange: {\n            color: ['#ebedf0', '#c6e48b', '#7bc96f', '#239a3b', '#196127']\n          }\n        },\n        series: [{\n          name: '能耗 (kWh)',\n          type: 'heatmap',\n          data: data.map(function (item) {\n            return [item[1], item[0], item[2]];\n          }),\n          label: {\n            show: false\n          },\n          emphasis: {\n            itemStyle: {\n              shadowBlur: 10,\n              shadowColor: 'rgba(0, 0, 0, 0.5)'\n            }\n          }\n        }]\n      };\n      energyHeatmapChart.setOption(option);\n    };\n\n    // 获取充电效率趋势数据\n    var fetchEfficiencyTrend = function fetchEfficiencyTrend() {\n      // 实际项目中应该从API获取数据\n      console.log(\"\\u83B7\\u53D6\".concat(trendTimeRange.value, \"\\u5929\\u7684\\u5145\\u7535\\u6548\\u7387\\u8D8B\\u52BF\\u6570\\u636E\"));\n    };\n\n    // 窗口大小变化时重新调整图表大小\n    var handleResize = function handleResize() {\n      efficiencyChart && efficiencyChart.resize();\n      stationStatusChart && stationStatusChart.resize();\n      orderStatusChart && orderStatusChart.resize();\n      energyHeatmapChart && energyHeatmapChart.resize();\n    };\n    onMounted(function () {\n      initCharts();\n      window.addEventListener('resize', handleResize);\n    });\n    onUnmounted(function () {\n      window.removeEventListener('resize', handleResize);\n      efficiencyChart && efficiencyChart.dispose();\n      stationStatusChart && stationStatusChart.dispose();\n      orderStatusChart && orderStatusChart.dispose();\n      energyHeatmapChart && energyHeatmapChart.dispose();\n    });\n    return {\n      stats: stats,\n      trendTimeRange: trendTimeRange,\n      fetchEfficiencyTrend: fetchEfficiencyTrend\n    };\n  }\n};","map":{"version":3,"names":["ref","reactive","onMounted","onUnmounted","ArrowUp","ArrowDown","echarts","name","components","setup","efficiencyChart","stationStatusChart","orderStatusChart","energyHeatmapChart","trendTimeRange","stats","stationCount","stationTrend","orderCount","orderTrend","avgEfficiency","efficiencyTrend","activeStations","activeTrend","initCharts","initEfficiencyTrendChart","initStationStatusChart","initOrderStatusChart","initEnergyHeatmap","chartDom","document","getElementById","init","option","tooltip","trigger","formatter","params","result","axisValue","forEach","param","marker","seriesName","value","legend","data","bottom","grid","left","right","top","containLabel","xAxis","type","boundaryGap","yAxis","min","max","series","setOption","radius","avoidLabelOverlap","itemStyle","borderRadius","borderColor","borderWidth","label","show","position","emphasis","fontSize","fontWeight","labelLine","color","hours","days","concat","parseInt","splitArea","visualMap","calculable","orient","inRange","map","item","shadowBlur","shadowColor","fetchEfficiencyTrend","console","log","handleResize","resize","window","addEventListener","removeEventListener","dispose"],"sources":["D:\\software_data\\Project\\Y0527\\CODE_BAK\\frontend\\src\\views\\Dashboard.vue"],"sourcesContent":["<template>\r\n  <div class=\"dashboard\">\r\n    <h1 class=\"page-title\">系统概览</h1>\r\n    \r\n    <!-- 关键指标卡片 -->\r\n    <div class=\"stat-cards\">\r\n      <el-card class=\"stat-card\" shadow=\"hover\">\r\n        <template #header>\r\n          <div class=\"card-header\">\r\n            <span>充电站总数</span>\r\n          </div>\r\n        </template>\r\n        <div class=\"card-value\">{{ stats.stationCount }}</div>\r\n        <div class=\"card-trend\" :class=\"{ 'up': stats.stationTrend > 0, 'down': stats.stationTrend < 0 }\">\r\n          <el-icon v-if=\"stats.stationTrend > 0\"><ArrowUp /></el-icon>\r\n          <el-icon v-else-if=\"stats.stationTrend < 0\"><ArrowDown /></el-icon>\r\n          <span>{{ Math.abs(stats.stationTrend) }}%</span>\r\n        </div>\r\n      </el-card>\r\n      \r\n      <el-card class=\"stat-card\" shadow=\"hover\">\r\n        <template #header>\r\n          <div class=\"card-header\">\r\n            <span>今日订单数</span>\r\n          </div>\r\n        </template>\r\n        <div class=\"card-value\">{{ stats.orderCount }}</div>\r\n        <div class=\"card-trend\" :class=\"{ 'up': stats.orderTrend > 0, 'down': stats.orderTrend < 0 }\">\r\n          <el-icon v-if=\"stats.orderTrend > 0\"><ArrowUp /></el-icon>\r\n          <el-icon v-else-if=\"stats.orderTrend < 0\"><ArrowDown /></el-icon>\r\n          <span>{{ Math.abs(stats.orderTrend) }}%</span>\r\n        </div>\r\n      </el-card>\r\n      \r\n      <el-card class=\"stat-card\" shadow=\"hover\">\r\n        <template #header>\r\n          <div class=\"card-header\">\r\n            <span>平均充电效率</span>\r\n          </div>\r\n        </template>\r\n        <div class=\"card-value\">{{ stats.avgEfficiency }}%</div>\r\n        <div class=\"card-trend\" :class=\"{ 'up': stats.efficiencyTrend > 0, 'down': stats.efficiencyTrend < 0 }\">\r\n          <el-icon v-if=\"stats.efficiencyTrend > 0\"><ArrowUp /></el-icon>\r\n          <el-icon v-else-if=\"stats.efficiencyTrend < 0\"><ArrowDown /></el-icon>\r\n          <span>{{ Math.abs(stats.efficiencyTrend) }}%</span>\r\n        </div>\r\n      </el-card>\r\n      \r\n      <el-card class=\"stat-card\" shadow=\"hover\">\r\n        <template #header>\r\n          <div class=\"card-header\">\r\n            <span>活跃充电站</span>\r\n          </div>\r\n        </template>\r\n        <div class=\"card-value\">{{ stats.activeStations }}</div>\r\n        <div class=\"card-trend\" :class=\"{ 'up': stats.activeTrend > 0, 'down': stats.activeTrend < 0 }\">\r\n          <el-icon v-if=\"stats.activeTrend > 0\"><ArrowUp /></el-icon>\r\n          <el-icon v-else-if=\"stats.activeTrend < 0\"><ArrowDown /></el-icon>\r\n          <span>{{ Math.abs(stats.activeTrend) }}%</span>\r\n        </div>\r\n      </el-card>\r\n    </div>\r\n    \r\n    <!-- 充电效率趋势图 -->\r\n    <el-card class=\"chart-card\" shadow=\"hover\">\r\n      <template #header>\r\n        <div class=\"card-header\">\r\n          <span>充电效率趋势</span>\r\n          <div class=\"header-actions\">\r\n            <el-radio-group v-model=\"trendTimeRange\" size=\"small\" @change=\"fetchEfficiencyTrend\">\r\n              <el-radio-button label=\"7\">7天</el-radio-button>\r\n              <el-radio-button label=\"30\">30天</el-radio-button>\r\n              <el-radio-button label=\"90\">90天</el-radio-button>\r\n            </el-radio-group>\r\n          </div>\r\n        </div>\r\n      </template>\r\n      <div id=\"efficiency-trend-chart\" class=\"chart-container\"></div>\r\n    </el-card>\r\n    \r\n    <div class=\"chart-row\">\r\n      <!-- 充电站状态分布 -->\r\n      <el-card class=\"chart-card\" shadow=\"hover\">\r\n        <template #header>\r\n          <div class=\"card-header\">\r\n            <span>充电站状态分布</span>\r\n          </div>\r\n        </template>\r\n        <div id=\"station-status-chart\" class=\"chart-container\"></div>\r\n      </el-card>\r\n      \r\n      <!-- 今日订单状态 -->\r\n      <el-card class=\"chart-card\" shadow=\"hover\">\r\n        <template #header>\r\n          <div class=\"card-header\">\r\n            <span>今日订单状态</span>\r\n          </div>\r\n        </template>\r\n        <div id=\"order-status-chart\" class=\"chart-container\"></div>\r\n      </el-card>\r\n    </div>\r\n    \r\n    <!-- 能耗分布热力图 -->\r\n    <el-card class=\"chart-card\" shadow=\"hover\">\r\n      <template #header>\r\n        <div class=\"card-header\">\r\n          <span>能耗分布热力图</span>\r\n        </div>\r\n      </template>\r\n      <div id=\"energy-heatmap\" class=\"chart-container\"></div>\r\n    </el-card>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nimport { ref, reactive, onMounted, onUnmounted } from 'vue';\r\nimport { ArrowUp, ArrowDown } from '@element-plus/icons-vue';\r\nimport * as echarts from 'echarts';\r\n\r\nexport default {\r\n  name: 'Dashboard',\r\n  components: {\r\n    ArrowUp,\r\n    ArrowDown\r\n  },\r\n  setup() {\r\n    // 图表实例\r\n    let efficiencyChart = null;\r\n    let stationStatusChart = null;\r\n    let orderStatusChart = null;\r\n    let energyHeatmapChart = null;\r\n    \r\n    // 时间范围\r\n    const trendTimeRange = ref('7');\r\n    \r\n    // 统计数据\r\n    const stats = reactive({\r\n      stationCount: 12,\r\n      stationTrend: 8.3,\r\n      orderCount: 156,\r\n      orderTrend: -2.5,\r\n      avgEfficiency: 91.2,\r\n      efficiencyTrend: 1.7,\r\n      activeStations: 10,\r\n      activeTrend: 0\r\n    });\r\n    \r\n    // 初始化图表\r\n    const initCharts = () => {\r\n      initEfficiencyTrendChart();\r\n      initStationStatusChart();\r\n      initOrderStatusChart();\r\n      initEnergyHeatmap();\r\n    };\r\n    \r\n    // 初始化充电效率趋势图\r\n    const initEfficiencyTrendChart = () => {\r\n      const chartDom = document.getElementById('efficiency-trend-chart');\r\n      efficiencyChart = echarts.init(chartDom);\r\n      \r\n      const option = {\r\n        tooltip: {\r\n          trigger: 'axis',\r\n          formatter: function(params) {\r\n            let result = params[0].axisValue + '<br/>';\r\n            params.forEach(param => {\r\n              result += param.marker + ' ' + param.seriesName + ': ' + param.value + '%<br/>';\r\n            });\r\n            return result;\r\n          }\r\n        },\r\n        legend: {\r\n          data: ['一号充电站', '二号充电站', '三号充电站', '四号充电站'],\r\n          bottom: 0\r\n        },\r\n        grid: {\r\n          left: '3%',\r\n          right: '4%',\r\n          bottom: '10%',\r\n          top: '3%',\r\n          containLabel: true\r\n        },\r\n        xAxis: {\r\n          type: 'category',\r\n          boundaryGap: false,\r\n          data: ['5-21', '5-22', '5-23', '5-24', '5-25', '5-26', '5-27']\r\n        },\r\n        yAxis: {\r\n          type: 'value',\r\n          name: '效率(%)',\r\n          min: 80,\r\n          max: 100\r\n      